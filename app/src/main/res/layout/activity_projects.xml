            android:layout_marginTop="151dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="128dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="127dp" android:layout_marginBottom="32dp"
            app:layout_constraintBottom_toTopOf="@+id/btnLab2" app:layout_constraintVertical_bias="0.39"
    />
    <Button
            android:text="Laboratorio 3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnLab2"
